var b = 6;

class Doughnut {
  cook() {
    print("Dunk in the fryer.");
    this.finish("sprinkles");
  }

  finish(ingredient) {
    println("Finish with " + ingredient);
  }
}

var b = Doughnut();
b.finish("cheese");

class Cruller < Doughnut {
  finish() {
    // No sprinkles, always icing.
    println(super.finish);
    super.finish("icing");
  }
}

var a = Cruller();
a.finish();
print(a.finish);