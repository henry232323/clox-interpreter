var SLEEP = 1;

fun function2(x) {
    yield [SLEEP, x];
    println("Slept ", x, " seconds");
    return x + 1;
}

fun finish() {
    var x = "First print";
    var r = yield;

    println("resumed with ", r);
    spawn(function2, 1);
    spawn(function2, 2);
    spawn(function2, 3);
    var future = spawn(function2, 4);

    println(future);
    println("Future is done: ", future.isReady());
    yield [SLEEP, 5];
    println("Future is done: ", future.isReady());
    println("Future result: ", future.getResult());

    return " ok!";
}

println("Clocky time", clock());

var gen = finish();
println("Function returned", gen);
