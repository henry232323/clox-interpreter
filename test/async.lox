var SLEEP = 1;

fun function2(x) {
    yield [SLEEP, x];
    println("Slept ", x, " seconds");
}

fun finish() {
    var x = "First print";
    var r = yield;

    println("resumed with ", r);
    spawn(function2, 1);
    spawn(function2, 2);
    spawn(function2, 3);
    spawn(function2, 4);
    return " ok!";
}

println("Clocky time", clock());

var gen = finish();
println("Function returned", gen);
