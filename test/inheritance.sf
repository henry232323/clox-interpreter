var b = 6;

class Doughnut {
  finish(ingredient: String) {
    println("Finish with " + ingredient);
  }

  cook() {
    println("Dunk in the fryer.");
    this.finish("sprinkles");
  }

  errorable!() {
    println("wowsers!");
  }
}

var b = Doughnut();
b.finish("cheese");

class Cruller < Doughnut {
  finish() {
    // No sprinkles, always icing.
    println(super.finish);
    super.finish("icing");
  }
}

var a = Cruller();
a.finish();
println(a.finish);