var SLEEP = 1;

fun function2(x: Number) {
    yield [SLEEP, x];
    println("Slept ", x);
    return true;
}

fun finish() {
    var x = "First print"
    var r = yield;

    println("resumed with ", r);
    spawn(fun () => function2(1))
    spawn(fun () => function2(2))
    spawn(fun () => function2(3))
    var future = spawn(fun () => function2(4))
    println(future, "wow!")

    println("Future is done: ", future.isReady())
    println("Future is done: ", future.isReady())
    println("Future result: ", future.getResult())

    return " ok!";
}

var gen = finish()
println("Function returned", gen)
